{
	"name": "@devtomio/sodium",
	"version": "0.3.0",
	"description": "Libsodium for Node.js",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/index.mjs",
	"typedocMain": "src/index.ts",
	"repository": "https://github.com/devtomio/sodium.git",
	"author": "Tomio <mail@tomio.codes>",
	"license": "MIT",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"napi": {
		"name": "sodium",
		"triples": {
			"defaults": true,
			"additional": [
				"x86_64-unknown-linux-musl",
				"aarch64-unknown-linux-gnu",
				"aarch64-apple-darwin",
				"aarch64-linux-android",
				"x86_64-unknown-freebsd",
				"aarch64-unknown-linux-musl",
				"armv7-unknown-linux-gnueabihf"
			]
		}
	},
	"scripts": {
		"lint": "eslint src tests --ext ts --fix",
		"format": "prettier --write . && cargo fmt",
		"docs": "typedoc",
		"test": "jest",
		"test:watch": "jest --watch",
		"update": "yarn upgrade-interactive",
		"ts:build": "tsc -b lib && gen-esm-wrapper dist/index.js dist/index.mjs",
		"rs:build": "napi build --platform --release",
		"rs:build-dev": "napi build --release",
		"rs:build-dev-windows": "cross-env SODIUM_LIB_DIR=\"C:\\Windows\\System32\" SODIUM_SHARED=1 yarn rs:build",
		"artifacts": "napi artifacts",
		"clean": "tsc -b lib --clean",
		"watch": "tsc -b lib -w",
		"prepublishOnly": "yarn ts:build && napi prepublish -t npm",
		"version": "napi version",
		"bench": "node -r @swc-node/register benchmark/bench.ts",
		"npm:publish": "node scripts/publish.mjs",
		"changelog": "node scripts/changelog.mjs"
	},
	"dependencies": {
		"@node-rs/helper": "^1.3.3",
		"tslib": "^2.4.0"
	},
	"devDependencies": {
		"@jsdevtools/npm-publish": "^1.4.3",
		"@napi-rs/cli": "^2.9.0",
		"@sapphire/eslint-config": "^4.3.6",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@swc-node/register": "^1.5.1",
		"@types/chloride": "^2.4.0",
		"@types/conventional-changelog": "^3",
		"@types/eslint": "^8",
		"@types/jest": "^27.5.2",
		"@types/libsodium-wrappers": "^0",
		"@types/node": "^17.0.21",
		"@types/prettier": "^2",
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"benny": "^3.7.1",
		"chloride": "^2.4.1",
		"conventional-changelog": "^3.1.25",
		"cross-env": "^7.0.3",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"gen-esm-wrapper": "^1.1.3",
		"jest": "^28.1.1",
		"jest-circus": "^28.1.1",
		"libsodium-wrappers": "^0.7.10",
		"lint-staged": "^12.5.0",
		"prettier": "^2.6.2",
		"semver-regex": "^4.0.4",
		"ts-jest": "^28.0.4",
		"ts-node": "^10.8.1",
		"tweetnacl": "^1.0.3",
		"typedoc": "^0.22.17",
		"typedoc-plugin-extras": "^2.2.3",
		"typedoc-plugin-mdn-links": "^1.0.6",
		"typescript": "^4.7.3"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"keywords": [
		"sodium",
		"libsodium",
		"typescript",
		"ts",
		"yarn",
		"napi",
		"napi-rs",
		"rust",
		"bindings"
	],
	"bugs": {
		"url": "https://github.com/devtomio/sodium/issues"
	},
	"homepage": "https://devtomio.github.io/sodium",
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.2.1",
	"prettier": "@sapphire/prettier-config"
}
